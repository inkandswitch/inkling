{
  "compilerOptions": {
    "esModuleInterop": true,
    "isolatedModules": true,
    "lib": ["ESNext", "DOM", "DOM.Iterable"],
    "module": "Preserve",
    "noEmit": true,
    "noImplicitAny": true,
    "strict": true,
    "noFallthroughCasesInSwitch": true,
    "target": "ESNext"
  },
  "include": ["src/**/*.ts"]
}
